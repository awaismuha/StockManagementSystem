@model List<StockManagementSystem.Web.Models.AuditLogViewModel>
@{
    ViewData["Title"] = "Audit Logs";
}
<h2>Audit Logs</h2>
<table class="table">
    <thead>
        <tr>
            <th>Action</th>
            <th>Entity</th>
            <th>EntityId</th>
            <th>User</th>
            <th>Timestamp</th>
            <th>Details</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var log in Model)
        {
            <tr>
                <td>@log.Action</td>
                <td>@log.Entity</td>
                <td>@log.EntityId</td>
                <td>@log.UserId</td>
                <td>@log.Timestamp</td>
                <td>@log.Details</td>
            </tr>
        }
    </tbody>
</table> 